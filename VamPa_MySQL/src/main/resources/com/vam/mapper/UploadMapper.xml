<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vam.mapper.UploadMapper">

	<select id="getUploadList" resultType="com.vam.model.UploadVO">
		
		select
			*
		from uploadfile
		where 1=1
		and freeboard_seq = #{freeboard_seq}
		
	</select>

</mapper>